{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": {
          "buildCommand": "apt-get update && apt-get install -y ffmpeg"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }